{
  "@context": [
    "https://www.w3.org/2018/credentials/v1",
    {
      "farmerContext": "https://raw.githubusercontent.com/shivakumar20014/s-rc/main/farmer-context.json",
      "FarmerRegistry": {
        "@id": "farmerContext:FarmerRegistry",
        "@type": "object",
        "required": [
          "farmerContext:title",
          "farmerContext:place"
        ],
        "properties": {
          "farmerContext:title": {
            "@id": "farmerContext:title",
            "@type": "xsd:string",
            "description": "Title of the registry entry"
          },
          "farmerContext:place": {
            "@id": "farmerContext:place",
            "@type": "xsd:string",
            "description": "Location of the farmer"
          },
          "farmerContext:email": {
            "@id": "farmerContext:email",
            "@type": "xsd:string",
            "description": "Farmer's email address (optional)"
          },
          "farmerContext:contact": {
            "@id": "farmerContext:contact",
            "@type": "xsd:string",
            "description": "Farmer's contact information (optional)"
          },
          "farmerContext:qualityCheck": {
            "@id": "farmerContext:qualityCheck",
            "@type": "xsd:string",
            "description": "Quality check status (optional)"
          }
        }
      }
    }
  ]
}
